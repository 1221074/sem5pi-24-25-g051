{
  "name": "bulletproof-nodejs",
  "version": "1.0.0",
  "description": "Bulletproof node.js",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "heroku-postbuild": "npm run build",
    "start": "nodemon",
    "inspect": "nodemon --inspect src/app.ts",
    "test:unit": "mocha -r ts-node/register test/unit/**/*.test.ts --timeout 10000",
    "test:integration": "mocha -r ts-node/register test/integration/**/*.test.ts --timeout 15000",
    "test:container": "mocha -r ts-node/register test/container/**/*.test.ts --timeout 20000",
    "test:e2e": "mocha -r ts-node/register test/e2e/**/*.test.ts --timeout 30000",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:container && npm run test:e2e",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint . --ignore-pattern .gitignore",
    "lint:js": "npm run lint:eslint",
    "lint:fix": "npm run lint:js -- --fix"
  },
  "keywords": [
    "boilerplay",
    "cron",
    "jobs",
    "js",
    "javascript",
    "typescript",
    "node",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "acorn": "^8.14.0",
    "acorn-jsx": "^5.3.2",
    "agenda": "^5.0.0",
    "agendash": "^4.0.0",
    "argon2": "^0.41.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.20.3",
    "celebrate": "^15.0.3",
    "chai": "^5.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "errorhandler": "^1.5.1",
    "event-dispatch": "^0.4.1",
    "eventemitter3": "^5.0.1",
    "express": "^4.21.1",
    "express-basic-auth": "^1.2.1",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "mongodb": "^6.11.0",
    "mongoose": "^8.8.3",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.7",
    "reflect-metadata": "^0.2.2",
    "shallow-equal-object": "^1.1.1",
    "sinon": "^19.0.2",
    "sinon-chai": "^4.0.0",
    "typedi": "^0.10.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.3",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "mocha": "^11.0.1",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  }
  
}
